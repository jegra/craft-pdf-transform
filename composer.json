{
    "name": "bymayo/pdf-transform",
    "description": "Transform a PDF page to an image (JPEG, PNG)",
    "type": "craft-plugin",
    "version": "5.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "pdf transform"
    ],
    "support": {
        "docs": "https://github.com/bymayo/craft-pdf-transform/blob/craft-5/README.md",
        "issues": "https://github.com/bymayo/craft-pdf-transform/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ByMayo",
            "homepage": "http://bymayo.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0",
        "spatie/pdf-to-image": "^2.3.0"
    },
    "autoload": {
        "psr-4": {
          "bymayo\\pdftransform\\": "src/"
        }
    },
    "extra": {
        "name": "PDF Transform",
        "handle": "pdf-transform",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bymayo/craft-pdf-transform/craft-5/CHANGELOG.md",
        "components": {
            "pdfTransformService": "bymayo\\pdftransform\\services\\PdfTransformService"
        },
        "class": "bymayo\\pdftransform\\PdfTransform"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main",
        "craftcms/phpstan": "dev-main"
    }
}
